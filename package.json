{
  "name": "@trnsprncy/ui",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "pnpm": "please use npm",
    "yarn": "please use npm",
    "bun": "please use npm",
    "npm": ">=10"
  },
  "workspaces": [
    "packages/cli"
  ],
  "scripts": {
    "dev": "next dev --turbo",
    "build": "contentlayer2 build && npm run build:registry && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "build:test": "npm run typecheck && npm run build:registry && next build && next start -p 3001",
    "build:registry": "npx tsx --tsconfig ./src/script/tsconfig.scripts.json ./src/script/registry-builder.ts",
    "typecheck": "contentlayer2 build && tsc --noEmit",
    "watch": "contentlayer2 dev && tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "cli": "npm run -w packages/cli cli",
    "init": "npm run -w packages/cli init"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    },
    "gitHooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@mdx-js/loader": "^3.0.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-virtual": "^3.0.2",
    "@trnsprncy/oss": "^0.0.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "contentlayer2": "^0.4.4",
    "eslint-config-next": "^14.1.2",
    "lucide-react": "^0.365.0",
    "mdast-util-toc": "^7.0.0",
    "mini-svg-data-uri": "^1.4.4",
    "next": "^14.1.2",
    "next-contentlayer2": "^0.4.4",
    "next-themes": "^0.2.1",
    "posthog-js": "^1.116.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.1",
    "react-wrap-balancer": "^1.1.0",
    "rehype-pretty-code": "^0.13.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-code-import": "^1.2.0",
    "remark-gfm": "^4.0.0",
    "rimraf": "^5.0.5",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss-animate": "^1.0.7",
    "unist-builder": "^4.0.0",
    "use-resize-observer": "^9.1.0",
    "vaul": "^0.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "husky": "^9.0.11",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
